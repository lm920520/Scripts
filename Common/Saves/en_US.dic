tr "game_spell_tooltip_UdyrPhoenixStance" = "<titleLeft>Phoenix Stance (@Level@)</titleLeft><titleRight>[@Hotkey@]</titleRight><subtitleLeft>@Cost@ Mana</subtitleLeft><subtitleRight>@Cooldown@ sec Cooldown</subtitleRight><mainText><font color='#FF9900'>Persistent Effect: </font>Burns enemies in front of Udyr on his first basic attack and every third attack after, dealing @Effect2Amount@ <font color='#99FF99'>(+@CharAbilityPower@)</font> magic damage.<br><br><font color='#FF9900'>Activation: </font>Unleashes pulsing waves of fire, dealing @Effect1Amount@ <font color='#99FF99'>(+@CharAbilityPower2@)</font> magic damage each second to nearby enemies for 5 seconds.</mainText>"
tr "game_spell_tooltip_UdyrTigerAttack" = ""
