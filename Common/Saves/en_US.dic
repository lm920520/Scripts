tr "game_spell_description_EvelynnE" = "Evelynn slashes her target twice, dealing damage with each hit. She then gains increased Attack Speed for a short duration."
tr "game_spell_description_EvelynnQ" = "Evelynn fires a line of spikes through an enemy, dealing damage to all enemies in its path."
tr "game_spell_description_EvelynnR" = "Evelynn summons spikes from the ground to deal damage and slow enemies in the area. She then gains a shield based on how many enemy champions were hit."
